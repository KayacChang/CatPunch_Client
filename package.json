{
  "name": "BaseSlot",
  "version": "1.0.0",
  "description": "Slot Game made by JavaScript",
  "scripts": {
    "dos2unix": "find src -type f -exec sed -i 's/^M$//' {} +",
    "clean": "rm -rf dist",
    "webpack": "webpack --config ./tools/build.js",
    "build": "npm run clean && NODE_ENV=production npm run webpack -- --env.mode=production",
    "watch": "npm run clean && NODE_ENV=development npm run webpack -- --env.mode=development --watch",
    "dev-server": "webpack-dev-server --config ./tools/build.js --host 0.0.0.0",
    "start": "NODE_ENV=development npm run dev-server -- --env.mode=development --open",
    "chrome": "open -a 'Google Chrome' http://localhost:3000",
    "local-server": "node ./server/main.js -- --PORT=3000 --mode=development",
    "server": "sleep 1 && npm run chrome & NODE_ENV=development npm run local-server"
  },
  "keywords": [],
  "author": "Kayac",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-flow": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "copy-webpack-plugin": "^5.0.2",
    "css-loader": "^2.1.1",
    "eslint": "^5.13.0",
    "eslint-config-google": "^0.12.0",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-flowtype": "^3.2.1",
    "express": "^4.16.4",
    "file-loader": "^3.0.1",
    "flow-bin": "^0.92.1",
    "html-webpack-plugin": "^4.0.0-beta.5",
    "mini-css-extract-plugin": "^0.5.0",
    "minimist": "^1.2.0",
    "sass-loader": "^7.1.0",
    "url-loader": "^1.1.2",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.2.3",
    "webpack-dev-middleware": "^3.5.2",
    "webpack-dev-server": "^3.2.1",
    "webpack-merge": "^4.2.1",
    "node-sass": "^4.12.0"
  },
  "dependencies": {
    "animejs": "^3.0.1",
    "axios": "^0.18.0",
    "core-js": "^3.0.1",
    "crypto-js": "^3.1.9-1",
    "eventemitter3": "^3.1.2",
    "howler": "^2.1.2",
    "lodash": "^4.17.11",
    "mathjs": "^5.9.0",
    "mobile-detect": "^1.4.3",
    "pixi-filters": "^2.7.1",
    "pixi.js": "^4.8.7",
    "pixi_fairygui": "^0.3.36",
    "ramda": "^0.26.1",
    "random-js": "^2.0.0",
    "xml-js": "^1.6.11",
    "zlibjs": "^0.3.1"
  }
}
